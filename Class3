"""
DocString

str > string > texto


Strings são textos dentros de aspas
"""
print(1234)

# Aspas simples

print('Hello, World')
print(1, '"Hello, World"')



# Aspas duplas

print(2, "'Hello, World'")
print("Hello, World")

# Escape 

print("Einsten disse: \"Pyton é bom\"")

#r

print(r"Hello \"World\"")
